<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    
   
   
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Demande_admin</servlet-name>
        <servlet-class>com.servlet.Demande_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddEtudiantServlet</servlet-name>
        <servlet-class>com.servlet.AjouterUtilisateurServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DaoServlet</servlet-name>
        <servlet-class>com.servlet.DaoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Demande_admin</servlet-name>
        <url-pattern>/Demande_admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddEtudiantServlet</servlet-name>
        <url-pattern>/AddEtudiantServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DaoServlet</servlet-name>
        <url-pattern>/DaoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    
    
</web-app>
